<div class="container-fluid mt-3">
  <h1 class="text-center text-danger">Interview List <br> 
    <a href="/download"><button type="button" class="btn btn-danger">Download</button></a>
  </h1>
  
  
  <table class="table table-striped">
        <thead>
          <tr>
            <th scope="col">S.No</th>
            <th scope="col">Company</th>
            <th scope="col">Batch</th>
            <th scope="col">Name</th>
            <th scope="col">Email</th>
            <th scope="col">Interview Date</th>
            <th scope="col">Result</th>
            <th scope="col">Update Result</th>
          </tr>
        </thead>
        <tbody>
          <%interviewData.forEach((student,index)=>{
          index++
          %>
          <tr>
            <th scope="row"><%=index%></th>
            <!-- <td></td> -->
            <td><%=student.company%></td>
            <td><%=student.studentId.batch%></td>
            <td><%=student.name%></td>
            <td><%=student.studentId.email%></td>
            <td><%=student.date%></td>
            <td><%=student.status%></td>
            <td><a href="/update-interview/<%=student._id%>"><button>Update Status</button></a></td>
          </tr>
          <%})%>
          
        </tbody>
      </table>
    </div>
</div>